Private Sub cmd_initComms_Click()\par
  Dim port As Integer\par
  Dim baud As Integer\par
  Dim parity As String\par
  Dim rspTimeout As Long\par
  Dim attempts As Integer\par
  Dim rc As Boolean\par
\par
  If (txt_port.Text = "") Or (IsNumeric(txt_port.Text) = False) Then\par
    txt_port.Text = "0"\par
  End If\par
  \par
  port = txt_port.Text\par
\par
  If (lst_baud.ListIndex = -1) Then\par
    lst_baud.ListIndex = 2\par
  End If\par
  \par
  baud = lst_baud.Text\par
\par
  If (lst_parity.ListIndex = -1) Then\par
    lst_baud.ListIndex = 0\par
  End If\par
  \par
  parity = Left(lst_parity.Text, 1)\par
\par
  If (txt_rspTimeout.Text = "") Or (IsNumeric(txt_rspTimeout.Text) = False) Then\par
    txt_rspTimeout.Text = "2000"\par
  End If\par
  \par
  rspTimeout = txt_rspTimeout.Text\par
\par
  If (txt_attempts.Text = "") Or (IsNumeric(txt_attempts.Text) = False) Then\par
    txt_attempts.Text = "1"\par
  End If\par
  \par
  attempts = txt_attempts.Text\par
\par
  clr_rsp_info\par
  \par
  If (autoSmplr.init_comms(port, baud, parity, rspTimeout, attempts) = False) Then\par
    MsgBox ("Problem with configuring Auto-Sampler comm port " & port & ". Verify that this port is available for usage."), vbCritical\par
  End If\par
End Sub\par
